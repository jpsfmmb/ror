<h1>Lista de alunos</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Nome</th>
    <th>Dtnascimento</th>
    <th>Serie</th>
    <th>Dtmatricula</th>
    <th>Mae</th>
  </tr>

<% @alunos.each do |aluno| %>
  <tr>
    <td><%=h aluno.id %></td>
    <td><%=h aluno.nome %></td>
    <td><%=h aluno.dtnascimento %></td>
    <td><%=h aluno.serie %></td>
    <td><%=h aluno.dtmatricula %></td>
    <td><%=h aluno.mae %></td>
    <td><%= link_to 'Show', aluno %></td>
    <td><%= link_to 'Edit', edit_aluno_path(aluno) %></td>
    <td><%= link_to 'Destroy', aluno, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New aluno', new_aluno_path %>
